{
  "name": "AutoCalendar",
  "version": "0.1",
  "description": "Ability to automatically link up calendars",
  "permissions": ["activeTab", "declarativeContent", "storage", "contextMenus"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/cal_icon.jpeg",
      "32": "images/cal_icon.jpeg",
      "48": "images/cal_icon.jpeg",
      "128": "images/cal_icon.jpeg"
    }
  },
  "icons": {
    "16": "images/cal_icon.jpeg",
    "32": "images/cal_icon.jpeg",
    "48": "images/cal_icon.jpeg",
    "128": "images/cal_icon.jpeg"
  },
  "manifest_version": 2
}